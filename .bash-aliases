# tree with 3 lvl nesting
alias sd='tree -F -C --dirsfirst -L 1'
alias sd2='tree -F -C --dirsfirst -L 2'
alias sd3='tree -F -C --dirsfirst -L 3'

# system
alias c='clear'
alias cf='touch'
alias md='mkdir'
alias rmd='rm -rf'

# dev
alias vc='code .'

# npm
alias n='npm install'
alias nid='npm install -D'
alias nrs='npm run start'
alias nrd='npm run dev'
alias nrb='npm run build'
alias nrl='npm run lint'

# yarn
alias y='yarn'
alias ya='yarn add'
alias yad='yarn add -D'
alias ys='yarn start'
alias yd='yarn dev'
alias yb='yarn build'
alias yl='yarn lint'